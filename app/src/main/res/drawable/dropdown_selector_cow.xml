<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_pressed="true">
		<shape>
			<solid android:color="@color/button_pressed_cow"/>
		</shape>
	</item>
	<item android:state_focused="true">
		<shape>
			<solid android:color="@color/panel_cow"/>
			<stroke
					android:color="@color/selection_cow"
					android:width="1dp"/>
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="@color/panel_cow"/>
		</shape>
	</item>
</selector >