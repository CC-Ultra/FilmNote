<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="fnote.snayper.com.filmsnote">
	<uses-permission
			android:name="android.permission.INTERNET" />
	<uses-permission
			android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	<application
			android:allowBackup="true"
			android:icon="@drawable/image_not_found"
			android:label="@string/app_name"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		<activity android:name="fnote.snayper.com.Activities.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="fnote.snayper.com.Activities.AddActivity"/>
		<activity android:name="fnote.snayper.com.Activities.EditActivity"/>
		<activity android:name="fnote.snayper.com.Activities.WebActivity"/>
		<activity android:name="fnote.snayper.com.Activities.ImageActivity"/>
	</application>
</manifest>
